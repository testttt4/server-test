{
	"name": "openfing-server",
	"description": "OpenFING Server",
	"scripts": {
		"start": "ts-node --compiler-options '{\"module\": \"commonjs\"}' --transpile-only scripts/start.ts",
		"scratch": "ts-node --project  tsconfig.json --compiler-options '{\"module\": \"commonjs\"}' --transpile-only scripts/scratch.ts",
		"build": "ts-node --project  tsconfig.json --compiler-options '{\"module\": \"commonjs\"}' --transpile-only scripts/build.ts",
		"run": "node dist",
		"build-and-run": "npm run build && npm run run",
		"ts-check": "tsc --noEmit",
		"lint-staged": "lint-staged",
		"deploy": "ts-node --compiler-options '{\"module\": \"commonjs\"}' scripts/deploy.ts",
		"clean": "rm -rf node_modules package-lock.json && npm i"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sangonz193/openfing-server.git"
	},
	"author": "Santiago Gonzalez",
	"license": "MIT",
